<IntlRateV2Request USERID="{user_id}">
    <Revision>2</Revision>
    <Package ID="0">
        <Pounds>{pounds}</Pounds>
        <Ounces>{ounces}</Ounces>
        <Machinable>True</Machinable>
        <MailType>Package</MailType>
        <GXG>
            <POBoxFlag>N</POBoxFlag>
            <GiftFlag>{gift}</GiftFlag>
        </GXG>
        <ValueOfContents>{value}</ValueOfContents>
        <Country>{country}</Country>
        <Container>{container}</Container>
        <Size>{size}</Size>
        <Width>{width}</Width>
        <Length>{length}</Length>
        <Height>{height}</Height>
        <Girth>{girth}</Girth>
        <OriginZip>{origin_zip}</OriginZip>
        <CommercialFlag>{commercial}</CommercialFlag>
        {insurance}
    </Package>
</IntlRateV2Request>